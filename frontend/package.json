{
  "name": "zara-challenge-frontend",
  "version": "1.0.0",
  "description": "Frontend for Zara Challenge",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build": "tsc -b && vite build --mode production",
